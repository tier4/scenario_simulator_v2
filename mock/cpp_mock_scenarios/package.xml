<?xml version="1.0"?>
<?xml-model href="http://download.ros.org/schema/package_format3.xsd" schematypens="http://www.w3.org/2001/XMLSchema"?>
<package format="3">
  <name>cpp_mock_scenarios</name>
  <version>4.3.1</version>
  <description>C++ mock scenarios</description>
  <maintainer email="masaya.kataoka@tier4.jp">masaya</maintainer>
  <license>Apache License 2.0</license>

  <buildtool_depend>ament_cmake</buildtool_depend>

  <buildtool_export_depend>ament_cmake_test</buildtool_export_depend>

  <depend>generate_parameter_library</depend>
  <depend>kashiwanoha_map</depend>
  <depend>simple_junit</depend>
  <depend>simple_sensor_simulator</depend>
  <depend>traffic_simulator</depend>

  <exec_depend>openscenario_visualization</exec_depend>
  <exec_depend>scenario_test_runner</exec_depend>
