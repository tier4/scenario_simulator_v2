<root>
    <BehaviorTree>
    <Fallback name="root">
        <FollowLane name="follow_lane"
            waypoints="{waypoints}"
            request="{request}"
            hdmap_utils="{hdmap_utils}"
            current_time="{current_time}"
            step_time="{step_time}"
            entity_status="{entity_status}"
            pedestrian_parameters="{pedestrian_parameters}"
            updated_status="{updated_status}"
            target_speed="{target_speed}"
            other_entity_status="{other_entity_status}"
            entity_type_list="{entity_type_list}"
            route_lanelets="{route_lanelets}"
            obstacle="{obstacle}"
            traffic_light_manager="{traffic_light_manager}"/>
        <WalkStraightAction name="walk_straight"
            waypoints="{waypoints}"
            request="{request}"
            hdmap_utils="{hdmap_utils}"
            current_time="{current_time}"
            step_time="{step_time}"
            entity_status="{entity_status}"
            pedestrian_parameters="{pedestrian_parameters}"
            updated_status="{updated_status}"
            target_speed="{target_speed}"
            other_entity_status="{other_entity_status}"
            entity_type_list="{entity_type_list}"
            route_lanelets="{route_lanelets}"
            obstacle="{obstacle}"
            traffic_light_manager="{traffic_light_manager}"/>
    </Fallback>
    </BehaviorTree>
</root>
